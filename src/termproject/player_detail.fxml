            <HBox spacing="10.0">
               <children>
                  <Label minWidth="62.0" text="Name:">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="FX_LABEL_PLAYER_NAME" text="Template Name" textAlignment="RIGHT">
                     <font>
                        <Font size="16.0" />
