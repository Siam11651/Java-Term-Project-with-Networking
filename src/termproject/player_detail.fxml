                  <Label text="Template Number">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox spacing="10.0">
               <children>
                  <Label minWidth="62.0" text="Salary:">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Label text="Template Salary">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
      <AnchorPane AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <Button fx:id="FX_BUTTON_ADD_TO_TRANSFER" mnemonicParsing="false" onAction="#Action_add_to_transfer" text="Add to Transfer Window" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
